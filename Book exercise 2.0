#include <stdio.h>

int main ()
    {
        
    float a, b, c, d, e;
    int x, y;
    
   x = 20;
   y = 2;
   
   
   a = (x--+ x * (x % y));
   b = (x--+ x * (x % y));
   c = (x--+ x * (x % 3));
   d = (--x + x * (x % 3));
   e = (--x + x * (x % x));


    printf ("RESPOSTAS\n\n");
    printf ("a) %.2f \n", a);
    printf ("b) %.2f \n", b);
    printf ("c) %.2f \n", c);
    printf ("d) %.2f \n", d);
    printf ("e) %.2f \n", e);
    
    
    return 0;
    
    }

